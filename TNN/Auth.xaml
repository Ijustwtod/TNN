<Window x:Class="TNN.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TNN"
        mc:Ignorable="d"
        Title="Авторизация" Height="450" Width="568" Background="#a3a3a3" ResizeMode="NoResize" WindowStartupLocation="CenterScreen" Icon="icon.png">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="25*"/>
            <ColumnDefinition Width="256*"/>
        </Grid.ColumnDefinitions>
        <Frame x:Name="AuthWindowFrame" NavigationUIVisibility="Hidden" Grid.ColumnSpan="2"/>
    </Grid>
</Window>
